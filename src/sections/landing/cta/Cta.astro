---
import StyledButton from "@/components/buttons/StyledButton";
import "./index.scss";
import ctaBg from "@/assets/photos/cta/cta-background.png";
---

<section class="cta">
    <div class="cta__top">
        <h2 class="cta__title">Приходите к нам!</h2>
    </div>
</section>
<div class="cta__bottom">
    <img class="cta__image" src={ctaBg.src} alt="Салон красоты" />
    <div class="cta__button"><StyledButton text="Записаться" href="/booking" client:only/></div>
</div>
